{
  "name": "quillpad",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "makemigrations": "run-script-os",
    "makemigrations:windows": ".\\venv\\Scripts\\python src\\api\\manage.py makemigrations blog users",
    "makemigrations:linux": "./venv/bin/python src/api/manage.py makemigrations blog users",
    "makemigrations:macos": "./venv/bin/python src/api/manage.py makemigrations blog users",
    "makemigrations:default": "npm run makemigrations:linux",
    "migrate": "run-script-os",
    "migrate:windows": ".\\venv\\Scripts\\python src\\api\\manage.py migrate",
    "migrate:linux": "./venv/bin/python src/api/manage.py migrate",
    "migrate:macos": "./venv/bin/python src/api/manage.py migrate",
    "migrate:default": "npm run migrate:linux",
    "createsuperuser": "run-script-os",
    "createsuperuser:windows": ".\\venv\\Scripts\\python src\\api\\manage.py createsuperuser",
    "createsuperuser:linux": "./venv/bin/python src/api/manage.py createsuperuser",
    "createsuperuser:macos": "./venv/bin/python src/api/manage.py createsuperuser",
    "createsuperuser:default": "npm run createsuperuser:linux",
    "start:frontend": "npm run dev",
    "start:backend": "run-script-os",
    "start:backend:windows": ".\\venv\\Scripts\\python src\\api\\manage.py runserver",
    "start:backend:linux": "./venv/bin/python src/api/manage.py runserver",
    "start:backend:macos": "./venv/bin/python src/api/manage.py runserver",
    "start:backend:default": "npm run start:backend:linux",
    "clean": "run-script-os",
    "clean:windows": "if exist dist rd /S /Q dist && if exist src\\api\\db.sqlite3 del src\\api\\db.sqlite3 && if exist src\\api\\blog\\migrations rd /S /Q src\\api\\blog\\migrations && if exist src\\api\\users\\migrations rd /S /Q src\\api\\users\\migrations && if exist venv rd /S /Q venv",
    "clean:linux": "rm -rf dist src/api/db.sqlite3 src/api/blog/migrations src/api/users/migrations venv",
    "clean:macos": "rm -rf dist src/api/db.sqlite3 src/api/blog/migrations src/api/users/migrations venv",
    "clean:default": "npm run clean:linux",
    "setup:venv": "run-script-os",
    "setup:venv:windows": "python -m venv venv",
    "setup:venv:linux": "python3 -m venv vvenv",
    "setup:venv:macos": "python3 -m venv venv",
    "setup:venv:default": "npm run setup:venv:linux",
    "setup:pip": "run-script-os",
    "setup:pip:windows": ".\\venv\\Scripts\\pip install -r requirements.txt",
    "setup:pip:linux": "./venv/bin/pip install -r requirements.txt",
    "setup:pip:macos": "./venv/bin/pip install -r requirements.txt",
    "setup:pip:default": "npm run setup:pip:linux",
    "setup:db": "npm run migrate",
    "setup": "npm install && npm run setup:venv && npm run setup:pip && npm run setup:db",
    "start:dev": "concurrently \"npm:start:frontend\" \"npm:start:backend\"",
    "build:prod": "npm run migrate && npm run build",
    "start:prod": "npm run start:backend",
    "start": "npm run start:dev",
    "init": "echo \"'init' is deprecated, use 'npm run setup' instead.\" && npm run setup"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "run-script-os": "^1.1.6",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@toast-ui/editor": "^3.2.2",
    "bootstrap": "^5.3.3",
    "dompurify": "^3.0.6",
    "jquery": "^3.7.1",
    "marked": "^12.0.1"
  },
  "author": "still_standing88",
  "license": "MIT"
}